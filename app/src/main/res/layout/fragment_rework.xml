<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


<!--    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout-->
<!--        android:id="@+id/swipeLayout"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="match_parent">-->


        <RelativeLayout
            android:id="@+id/rel0"
            android:paddingBottom="@dimen/padding_all"
            android:paddingEnd="@dimen/padding_all"
            android:paddingStart="@dimen/padding_all"
            android:clipToPadding="false"
            android:clipChildren="false"
            tools:background="@color/mainBackground"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <RelativeLayout
                android:id="@+id/rel1"
                android:layout_marginTop="@dimen/marginTop5ForLayouts"
                android:layout_alignParentTop="true"
                style="@style/relSearchMainEdi">

                <EditText
                    android:id="@+id/serialEdi"
                    style="@style/searchMainEdi"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:hint="@string/serial" />

                <ImageView
                    android:id="@+id/serialClearImg"
                    style="@style/imgClose" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rel2"
                style="@style/relSearchMainEdi"
                android:layout_below="@+id/rel1">

                <EditText
                    android:id="@+id/invType"
                    style="@style/searchMainEdi"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="true"
                    android:focusable="false"
                    android:gravity="left|center"
                    android:hint="@string/ownerCode" />

                <ImageView
                    android:id="@+id/invTypeImg"
                    style="@style/imgClose" />


            </RelativeLayout>



            <ProgressBar
                android:id="@+id/progressBar"
                style="@style/progressStyle"
                android:background="@null"
                android:padding="@dimen/paddingOfFragmentProgress"
                tools:visibility="gone" />


            <include
                android:id="@+id/printTv"
                layout="@layout/layout_print_tv" />


        </RelativeLayout>

<!--    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>-->



</layout>
