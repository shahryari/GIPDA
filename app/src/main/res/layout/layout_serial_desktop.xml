<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">



    <RelativeLayout
        android:padding="@dimen/padding_all"
        android:layout_width="match_parent"
        tools:background="@color/mainBackground"
        tools:backgroundTint="@color/mainBackground"
        android:layout_height="match_parent"
        tools:showIn="@layout/fragment_desktop">

        <RelativeLayout
            android:id="@+id/rel1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/heightOfDashboard"
            android:paddingStart="@dimen/padding_rel_loginToolbar"
            android:paddingEnd="@dimen/padding_rel_loginToolbar">


            <TextView
                android:id="@+id/appDescriptionTv"
                style="@style/desktop_tv"
                android:layout_alignParentBottom="true"
                android:text="@string/appDesc" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rel2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rel1"
            android:layout_marginTop="@dimen/desktopMargintTop"
            android:clipToPadding="false"
            android:padding="@dimen/padding_rel_login">

            <View
                android:id="@+id/view"
                android:layout_width="@dimen/_1sdp"
                android:layout_height="@dimen/_1sdp"
                android:layout_centerInParent="true" />

            <RelativeLayout
                android:id="@+id/relSerialPutaway"
                style="@style/desktop_rel"
                android:layout_alignParentTop="true"
                android:layout_toStartOf="@id/view">



                <TextView
                    style="@style/serial_desktop_tv"
                    android:id="@+id/receivingTitle"
                    android:layout_centerInParent="true"
                    android:text="@string/serailPutaway" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relSerialPutawayAssign"
                style="@style/desktop_rel"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@id/view">

                <TextView
                    style="@style/serial_desktop_tv"
                    android:id="@+id/putawayTitle"
                    android:layout_centerInParent="true"
                    android:text="@string/serialPutawayAssign" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relSerialTransfer"
                style="@style/desktop_rel"
                android:layout_width="match_parent"
                android:layout_height="@dimen/desktop_rel_height2"
                android:layout_below="@+id/relSerialPutaway"
                android:layout_alignStart="@+id/relSerialPutaway"
                android:layout_alignEnd="@id/relSerialPutawayAssign"
                android:layout_marginStart="0dp"
                android:layout_marginEnd="0dp">

                <TextView
                    style="@style/serial_desktop_tv"
                    android:id="@+id/waitForLoadingTitle"
                    android:layout_centerInParent="true"
                    android:text="@string/serial_location_transfer" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relSerialPicking"
                style="@style/desktop_rel"
                android:layout_below="@+id/relSerialTransfer"
                android:layout_toStartOf="@id/view">


                <TextView
                    style="@style/serial_desktop_tv"
                    android:id="@+id/pickingTitle"
                    android:layout_centerInParent="true"
                    android:text="@string/serialPicking" />



            </RelativeLayout>



            <RelativeLayout
                android:id="@+id/relSerialShipping"
                style="@style/desktop_rel"
                android:layout_below="@+id/relSerialTransfer"
                android:layout_toEndOf="@id/view">


                <TextView
                    style="@style/serial_desktop_tv"
                    android:id="@+id/shippingTitle"
                    android:layout_centerInParent="true"
                    android:text="@string/shipping_serialbase" />

            </RelativeLayout>



        </RelativeLayout>


        <RelativeLayout
            android:layout_width="@dimen/_200sdp"
            android:layout_height="@dimen/_200sdp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">



            </RelativeLayout>

            <TextView
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>


        </RelativeLayout>





    </RelativeLayout>
</layout>