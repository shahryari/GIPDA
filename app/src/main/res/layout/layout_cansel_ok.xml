<?xml version="1.0" encoding="utf-8"?>
<layout>
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:padding="@dimen/marginTop_flag"
        android:layout_height="@dimen/canselConfirmRel">

        <TextView
            android:id="@+id/cansel"
            style="@style/btnSubmitCansel"
            android:layout_alignParentStart="true"
            android:layout_marginStart="0dp" />

        <TextView
            android:id="@+id/ok"
            style="@style/btnSubmitCansel"
            android:backgroundTint="@color/grayAA"
            android:layout_toEndOf="@+id/cansel"
            android:background="@drawable/shape_background_rect_gray_3"
            android:text="@string/ok" />



    </RelativeLayout>
</layout>
