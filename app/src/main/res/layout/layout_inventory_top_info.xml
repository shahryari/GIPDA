<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/rel1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingStart="@dimen/paddingSerialInfoLayout"
            android:paddingEnd="@dimen/paddingSerialInfoLayout">

            <TextView
                android:id="@+id/tv1"
                style="@style/pattern_receieve_tv1"
                android:text="@string/tools_Panel_HV36MNQ1" />
            <TextView
                android:id="@+id/tv2"
                style="@style/pattern_receieve_tv1"
                android:text="@string/tools_Panel_HV36MNQ1" />
            <TextView
                android:id="@+id/tv3"
                style="@style/pattern_receieve_tv1"
                android:layout_marginBottom="@dimen/marginBottomInventoryTop"
                android:text="@string/tools_Panel_HV36MNQ1" />


        </LinearLayout>

        <RelativeLayout
            android:id="@+id/rel2"
            android:layout_below="@id/rel1"
            android:layout_marginTop="@dimen/marginTop5ForLayouts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/locationDestiny"
                style="@style/ediForSmallDialog"
                android:layout_centerVertical="true"
                android:layout_marginBottom="0dp"
                android:gravity="start|center"
                android:hint="@string/destinationLocation" />

            <ImageView
                android:id="@+id/clearImgLocation"
                style="@style/imgClose"
                android:layout_marginEnd="@dimen/clearImgMarginEnd"
                android:elevation="@dimen/clearImgElevation" />

        </RelativeLayout>

        <include
            layout="@layout/layout_white_field"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/rel2" />


    </RelativeLayout>
</layout>
